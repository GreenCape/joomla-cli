{
  "name": "greencape/joomla-cli",
  "description": "A tool for managing Joomla! installations from the command line.",
  "homepage": "https://github.com/GreenCape/joomla-cli",
  "license": "MIT",
  "authors": [
    {
      "name": "Niels Braczek",
      "email": "nbraczek@bsds.de",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/GreenCape/joomla-cli/issues"
  },
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-dom": "*",
    "sebastian/finder-facade": "*",
    "sebastian/git": "*",
    "symfony/console": "~2",
    "league/flysystem": "^1.0",
    "league/flysystem-ziparchive": "^1.0",
    "theseer/autoload": "^1.25",
    "phpunit/phpunit": "^8.3",
    "greencape/joomla-manifest": "^0.4",
    "pdepend/pdepend": "^2.5",
    "phpmd/phpmd": "^2.7",
    "sebastian/phpcpd": "^4.1",
    "greencape/coding-standards": "^1.0",
    "mayflower/php-codebrowser": "^2.0",
    "nikic/php-parser": "^4.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "codeclimate/php-test-reporter": "dev-master",
    "league/flysystem-memory": "^1.0",
    "twig/twig": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "GreenCape\\JoomlaCLI\\": "src"
    },
    "classmap": [
      "src/Driver"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "UnitTest\\": "tests"
    }
  },
  "bin": [
    "joomla"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
