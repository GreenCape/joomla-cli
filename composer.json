{
  "name": "greencape/joomla-cli",
  "description": "A tool for managing Joomla! installations from the command line.",
  "homepage": "https://github.com/GreenCape/joomla-cli",
  "license": "MIT",
  "authors": [
    {
      "name": "Niels Braczek",
      "email": "nbraczek@bsds.de",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/GreenCape/joomla-cli/issues"
  },
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-dom": "*",
    "symfony/console": "*",
    "league/flysystem": "*",
    "league/flysystem-ziparchive": "*",
    "theseer/autoload": "*",
    "phpunit/phpunit": "*",
    "greencape/joomla-manifest": "dev-master",
    "pdepend/pdepend": "*",
    "phpmd/phpmd": "*",
    "sebastian/phpcpd": "*",
    "mayflower/php-codebrowser": "*",
    "nikic/php-parser": "*",
    "squizlabs/php_codesniffer": "*",
    "vlucas/phpdotenv": "*",
    "twig/twig": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "codeclimate/php-test-reporter": "dev-master",
    "league/flysystem-memory": "*"
  },
  "autoload": {
    "psr-4": {
      "GreenCape\\JoomlaCLI\\": "src"
    },
    "classmap": [
      "src/Driver"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "UnitTest\\": "tests"
    }
  },
  "bin": [
    "joomla"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
